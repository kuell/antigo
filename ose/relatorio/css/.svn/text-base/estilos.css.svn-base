BODY {
	MARGIN: 0px; BACKGROUND-COLOR: #f2f3f5;
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background-image: url(img/fundo.gif);

}
BODY {
	FONT-SIZE: 11px; COLOR: #666666; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif
}
TD {
	FONT-SIZE: 11px; COLOR: #666666; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif
}
TH {
	FONT-SIZE: 11px; COLOR: #666666; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif
}
A {
	FONT-SIZE: 11px; COLOR: #333333
}
A:link {
	TEXT-DECORATION: none
}
A:visited {
	COLOR: #4f5b6a; TEXT-DECORATION: none
}
A:hover {
	COLOR: #000000; TEXT-DECORATION: underline
}
A:active {
	COLOR: #000000; TEXT-DECORATION: underline blink
}
.formularios {
	FONT-WEIGHT: normal;
	FONT-SIZE: 10px;
	COLOR: #666666;
	FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif;
	BACKGROUND-COLOR: #ffffff;
	border: 1px solid #C03532;
}
.formOver {
	BORDER-RIGHT: #cccccc 1px inset; BORDER-TOP: #cccccc 1px inset; FONT-WEIGHT: normal; FONT-SIZE: 10px; BORDER-LEFT: #cccccc 1px inset; COLOR: #ffffff; BORDER-BOTTOM: #cccccc 1px inset; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif; BACKGROUND-COLOR: #a2d823
}
.style1 {
	FONT-WEIGHT: bold; COLOR: #C03532
}
.formulario2 {
	FONT-WEIGHT: normal;
	FONT-SIZE: 11px;
	COLOR: #FFFFFF;
	FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif;
	BACKGROUND-COLOR: #C03532;
	border: 1px solid #D4D0C8;
}
.style2 {

	FONT-WEIGHT: bold; COLOR: #333333
}

