.back_button {
  float: left;
  text-align: center;
  font-family      : Verdana, Arial, Helvetica, sans-serif;
  font-size: 10px;
  font-weight: bold;
  border: 3px double #cccccc;
  width: 90px;
  background: url(../images/button_bg.png) repeat-x;
  padding: 0px 10px;
  line-height: 20px;
  margin: 1px;
}

